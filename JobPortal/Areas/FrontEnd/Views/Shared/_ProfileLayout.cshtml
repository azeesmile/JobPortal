<!DOCTYPE html>
<!--[if IE 7]><html class = "ie7"><![endif]-->
<!--[if IE 8]><html class = "ie8"><![endif]-->
<!--[if IE 9]><html class = "ie9"><![endif]-->
<!--[if !(IE 7) | !(IE 8) | !(IE 9) ]><!-->
<html>
<!--<![endif]-->
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/scripts")
    <!--[if IE]><script src="js/html5.js"></script><link rel="stylesheet" type="text/css" href="css/ie.css"><![endif]-->
</head>
<body>
        
            <header>
                <div class="site-header">
                    @Html.Partial("~/Views/Partial/sitetitle.cshtml")
                    <!-- .site-title -->
                    @Html.Partial("~/Views/Partial/sitenav.cshtml")
                    <!-- .site-nav -->
                    @Html.Partial("~/Views/Partial/userarea.cshtml")
                    <!-- .user-area -->
                </div>
                <!-- .site-header -->
            </header>
    <div class="main clearfix">
        <div class="wrapper">
            @RenderBody()
        </div>
    </div>
        <!-- .main -->
        <footer>
            <div class="site-footer">
                @Html.Partial("~/Views/Partial/sitetitle.cshtml")
                <!-- .site-title -->
                @Html.Partial("~/Views/Partial/footer-nav.cshtml")
                <!-- .site-nav -->
                @Html.Partial("~/Views/Partial/sociallinks.cshtml")
                <!-- .links -->
            </div>
            <!-- .site-footer -->
        </footer>
        <!-- footer -->
    
        <!-- .wrapper -->
        @Scripts.Render("~/bundles/scripts.js")
</body>
</html>